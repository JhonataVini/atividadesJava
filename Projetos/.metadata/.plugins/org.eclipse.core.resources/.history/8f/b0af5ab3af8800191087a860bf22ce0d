public class Aporte {

	private double valor;
	public Projeto projeto;
	
	
	public void setValor(double v) {
		
		 this.valor = v;
		
	}
	
	public double valor() {
				
		return this.valor;
		
	}
	
	public String getTipoStr() {
		
		return "Nunca Execulta";
	}
}
