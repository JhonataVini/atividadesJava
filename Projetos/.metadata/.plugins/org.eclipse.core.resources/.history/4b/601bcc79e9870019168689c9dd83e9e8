package entidade;

/**
 * @author jhonata.lopes.a 
 */
public class Aporte {

	private double valor;
	//private int tipo; //1- Doacao 2- Investimento
	public Projeto projeto;
	
	
	public Aporte(int tipo, double valor) {
		this.valor = valor;
	}
	
	
	public double valor() {
				
		return this.valor;
		
	}
	
	public double saldo() {
			
		return this.valor;
	}
	
	
}
