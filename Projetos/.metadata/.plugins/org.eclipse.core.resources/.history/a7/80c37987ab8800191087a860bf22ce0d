package entidade;

/**
 * @author jhonata.lopes.a 
 */
public class Aporte {

	private double valor;
	public Projeto projeto;
	
	
	public Aporte( double valor) {
		this.valor = valor;
	}
	
	
	public double valor() {
				
		return this.valor;
		
	}
	
	public double saldo() {
			
		return this.valor;
	}
	
	public String getTipoStr() {
		
		return "Nunca Execulta";
	}
}
